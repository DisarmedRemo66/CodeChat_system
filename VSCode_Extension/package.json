{
    "name": "codechat_vsccode_extension",
    "displayName": "The Visual Studio Code CodeChat Extension",
    "description": "This embeds CodeChat in the Visual Studio Code Editor.",
    "keywords": [
        "literate programming",
        "CodeChat",
        "Visual Studio Code extension"
    ],
    "homepage": "https://github.com/bjones1/A_modular_Plugin-CodeChat",
    "license": "GPL-3.0-or-later",
    "publisher": "todo",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.41.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.codeChat"
    ],
    "main": "out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.codeChat",
                "title": "CodeChat"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "tslint -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.6",
        "@types/node": "^8.10.25",
        "@types/vscode": "^1.41.0",
        "@types/node-int64": "^0.4.29",
        "@types/thrift": "^0.10.9",
        "glob": "^7.1.4",
        "mocha": "^6.1.4",
        "source-map-support": "^0.5.12",
        "tslint": "^5.16.0",
        "typescript": "^3.5.1",
        "vscode-test": "^1.3.0"
    },
    "dependencies": {
        "thrift": "^0.12.0"
    }
}
